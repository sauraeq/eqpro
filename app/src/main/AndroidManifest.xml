<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.equalinfotechuser">

    <uses-permission
        android:name="android.permission.ACCESS_COARSE_LOCATION"
        android:maxSdkVersion="22" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoss"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoss"
        android:supportsRtl="true"
        android:theme="@style/Theme.EqulaInfotech"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Activity.Attendence.Holidays.HolidaysActivity"
            android:exported="false" />
        <activity
            android:name=".MonthlyReports"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Attendence.MonthlyReport"
            android:exported="false" />
        <activity
            android:name=".Activity.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".Activity.HomeActivityStudent"
            android:exported="true" />
        <activity
            android:name=".Activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Attendence.MonthlyReports" />
    </application>

</manifest>